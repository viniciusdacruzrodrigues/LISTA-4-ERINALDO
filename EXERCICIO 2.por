programa {
  funcao inicio() {
  inteiro numero
  
  escreva("Digite um valor: ")
  leia(numero)

    se(numero > 0){
    escreva("Valor positivo")
    }senao se(numero < 0){
    escreva("Valor negativo")
    }senao{
    se(numero == 0)
    escreva("Zero")
    }

    
  }
}
